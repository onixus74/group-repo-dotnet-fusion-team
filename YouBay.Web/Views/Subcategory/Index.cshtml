            @Html.DisplayFor(modelItem => item.assistantAvatarImage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.categoryDisplayPriority)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.categoryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.isActive)

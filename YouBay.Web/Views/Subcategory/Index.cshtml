@model IEnumerable<YouBay.Domain.Entities.Subcategory>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.assistantAvatarImage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.categoryDisplayPriority)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.categoryName)
